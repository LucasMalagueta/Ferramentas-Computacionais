<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.botekofx.TabelaTipoPagamentoController">
    <children>
        <TableView fx:id="tableView" layoutX="14.0" layoutY="14.0" prefHeight="320.0" prefWidth="572.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="14.0">
            <columns>
                <TableColumn fx:id="colId" prefWidth="75.0" text="ID" />
                <TableColumn fx:id="colDescricao" prefWidth="496.0" text="Descrição" />
            </columns>
        </TableView>
        <HBox layoutX="14.0" layoutY="348.0" prefHeight="38.0" prefWidth="572.0" spacing="10.0" AnchorPane.bottomAnchor="14.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0">
            <children>
                <Button mnemonicParsing="false" onAction="#onNovo" prefHeight="30.0" prefWidth="80.0" text="Novo" />
                <Button fx:id="btAlterar" mnemonicParsing="false" onAction="#onAlterar" prefHeight="30.0" prefWidth="80.0" text="Alterar" />
                <Button fx:id="btApagar" mnemonicParsing="false" onAction="#onApagar" prefHeight="30.0" prefWidth="80.0" text="Apagar" />
                <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                    <children>
                        <Button mnemonicParsing="false" onAction="#onFechar" prefHeight="30.0" prefWidth="80.0" text="Fechar" />
                    </children>
                </HBox>
            </children>
            <padding>
                <Insets top="4.0" />
            </padding>
        </HBox>
    </children>
</AnchorPane>